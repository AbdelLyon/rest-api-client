{
  "name": "rest-api-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "description": "Xefi rest-api-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/AbdelLyon/rest-api-client.git"
  },
  "author": "DailyApps",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.es.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./auth": {
      "import": "./dist/esm/auth/index.js",
      "types": "./dist/esm/auth/inex.d.ts"
    },
    "./http": {
      "import": "./dist/esm/http/index.es.js",
      "types": "./dist/esm/http/index.d.ts"
    },
    "./mutation": {
      "import": "./dist/esm/mutation/index.es.js",
      "types": "./dist/esm/mutation/index.d.ts"
    },
    "./query": {
      "import": "./dist/esm/query/index.es.js",
      "types": "./dist/esm/query/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts"
      ]
    }
  },
  "scripts": {
    "clean": "rimraf node_modules dist *.log && pnpm install",
    "build": "pnpm tsc -b && vite build",
    "test": "vitest",
    "test:cov": "vitest --coverage",
    "lint": "eslint .",
    "preview": "vite preview",
    "reset": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@tanstack/vite-config": "^0.2.0",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "cookies-next": "^5.1.0",
    "eslint": "^9.25.0",
    "jiti": "^2.4.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tanstack/eslint-config": "^0.1.0",
    "@types/node": "^22.10.7",
    "@vitest/coverage-v8": "^2.1.8",
    "globals": "^15.14.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  }
}
